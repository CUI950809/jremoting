<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<import resource="classpath:jremoting-context.xml"/>
	
	<bean id="testService" class="com.github.jremoting.spring.JRemotingConsumerBean">
		<property name="interfaceName" value="com.github.jremoting.core.test.TestService" />
		<property name="version" value="1.0" />
		<property name="rpcClient" ref="rpcClient" />
	</bean>
	
</beans>